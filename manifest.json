{
  "manifest_version": 3,
  "name": "Copy Tab Title",
  "version": "1.0",
  "description": "Copies the title of the current tab to clipboard",
  "permissions": ["activeTab", "clipboardWrite", "scripting"],
  "action": {
    "default_icon": "icon.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}